{
"AWSEBDockerrunVersion": 2,
"containerDefinitions":[
  {
	"name": "client",
	"image": "rickopsourced/multi-client",
	"hostname": "client",
	"essential": false
  },
  {
	"name": "server",
	"image": "rickopsourced/multi-server",
	"hostname": "api",
	"essential": false
  },
  {
	"name": "worker",
	"image": "rickopsourced/multi-worker",
	"hostname": "worker",
	"essential": false
  },
  {
	"name": "nginx",
	"image": "rickopsourced/multi-nginx",
	"hostname": "nginx",
	"essential": true,
	"portMappings": [
		{
			"hostPort":80,
			"containterPort":80
		}
	],
	"links": ["client", "server"]
  }
]
}
